class Solution {
    public int countOperations(int num1, int num2) {
        int n=0;
        while(num1!=0&&num2!=0){
            if(num1>num2){
                num1=num1-num2;
                n++;
            }
            else{
                num2=num2-num1;
                n++;
            }
            
        }
        return n;
    }
}
