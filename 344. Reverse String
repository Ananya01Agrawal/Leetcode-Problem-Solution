class Solution {
    public void reverseString(char[] s) {
        int n=s.length;
        
        if(n==0){
            System.out.println(s);
        }
        else{
            for(int i=0;i<(n/2);i++){
                char temp=s[i];
                s[i]=s[n-1-i];
                s[n-1-i]=temp;
            }
            System.out.println(s);
        }
    }
}
